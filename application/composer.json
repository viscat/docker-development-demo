{
  "name": "viscat/demo",
  "type": "project",
  "license": "proprietary",
  "description": "Demo project",
  "require": {
    "php": "^7.1.3",
    "symfony/console": "^3.4",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^3.4",
    "symfony/yaml": "^3.4"
  },
  "require-dev": {
    "symfony/dotenv": "^3.3"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "make cache-warmup": "script",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*",
    "symfony/twig-bundle": "<3.3",
    "symfony/debug": "<3.3"
  },
  "extra": {
    "symfony": {
      "id": "01BMKSVNT708S9Q6CG2EVGZF7G",
      "allow-contrib": false
    }
  }
}
